@using FoodCorner.Areas.Client.ViewModels.Blog
@model List<TagListItemViewModel>

<div class="p-t-50">
	<h4 class="mtext-112 cl2 p-b-27">
		Tags
	</h4>

	<div class="flex-w m-r--5">
        @foreach (var item in Model)
        {
            <input type="hidden" value="@item.Id" />
            <a asp-route="client-blog-filter" class="select-blog-tag flex-c-m stext-107 cl6 size-301 bor7 p-lr-15 hov-tag1 trans-04 m-r-5 m-b-5">
                @item.Title
            </a>
        }
	</div>
</div>